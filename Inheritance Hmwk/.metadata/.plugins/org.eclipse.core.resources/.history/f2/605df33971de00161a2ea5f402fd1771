package inheritance;
import java.util.*;

public class Billfold {
	ArrayList<Card> cards = new ArrayList<Card>();
	
	public Billfold() {
		for (int i=0;i<2;i++) {
			cards.add(null);
		}
	}
	
	void addCard(Card card) {
		for (int i=0;i<cards.size();i++) {
			if (cards.get(i) == null) {
				cards.add(i, card);
				break;
			}
		}
	}

	String formatCards() {
		
	}
}
